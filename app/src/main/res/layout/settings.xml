            android:text="Help"
            android:textColor="#000"
            android:textSize="@dimen/TextSize16"
            app:layout_constraintStart_toEndOf="@+id/imageView5"
            app:layout_constraintTop_toBottomOf="@+id/view4" />

        <TextView
            android:id="@+id/textView13000"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="24dp"
            android:layout_marginTop="2dp"
            android:layout_marginEnd="16dp"
            android:text="Get help about using Solved'out"
            android:textColor="#999999"
            android:textSize="@dimen/TextSize14"
            app:layout_constraintStart_toEndOf="@+id/imageView5"
            app:layout_constraintTop_toBottomOf="@+id/textView10000" />


        <View
            android:id="@+id/view5"
            android:layout_width="match_parent"
            android:layout_height="1dp"

            android:layout_marginTop="16dp"
            android:background="#0D000000"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView13000"

            />

        <Button
            android:id="@+id/Help"
            android:layout_width="match_parent"
            android:layout_height="69dp"
            android:layout_marginTop="5dp"
            android:background="#00000000"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/view4"

            />


        <!--Help Button-->


        <ImageView
            android:id="@+id/imageView500"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="16dp"
            android:src="@drawable/lock_"
            app:layout_constraintBottom_toBottomOf="@+id/textView130000"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="@+id/textView10000000" />

        <TextView
            android:id="@+id/textView10000000"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="24dp"
            android:layout_marginTop="16dp"
            android:fontFamily="sans-serif-medium"
            android:text="Reset Password"
            android:textColor="#000"
            android:textSize="@dimen/TextSize16"
            app:layout_constraintStart_toEndOf="@+id/imageView500"
            app:layout_constraintTop_toBottomOf="@+id/view5" />

        <TextView
            android:id="@+id/textView130000"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="24dp"
            android:layout_marginTop="2dp"
            android:layout_marginEnd="16dp"
            android:text="Reset your password"
            android:textColor="#999999"
            android:textSize="@dimen/TextSize14"
            app:layout_constraintStart_toEndOf="@+id/imageView500"
            app:layout_constraintTop_toBottomOf="@+id/textView10000000" />


        <View
            android:id="@+id/view200"
            android:layout_width="match_parent"
            android:layout_height="1dp"

            android:layout_marginTop="16dp"
            android:background="#0D000000"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView130000"

            />

        <Button
            android:id="@+id/ResetPassword"
            android:layout_width="match_parent"
            android:layout_height="69dp"
            android:layout_marginTop="5dp"
            android:background="#00000000"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/view5"

            />

        <!--Reset Password-->
        <TextView
            android:id="@+id/textView14"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="16dp"
