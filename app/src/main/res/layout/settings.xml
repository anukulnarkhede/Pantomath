<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".StudentProfile.Settings"
    tools:ignore="HardcodedText"
    >

    <com.google.android.material.appbar.AppBarLayout
        android:id="@+id/appBarLayout"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/white"
        android:elevation="4dp"

        app:elevation="4dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">


        <androidx.appcompat.widget.Toolbar
            android:id="@+id/SettingsToolBar"
            android:layout_width="match_parent"
            android:layout_height="?attr/actionBarSize"
            android:background="#1EF5F5F5"
            android:elevation="4dp"
            app:elevation="4dp">

            <TextView

                android:id="@+id/toolbar_titleSettings"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:fontFamily="sans-serif-medium"
                android:text="Settings"
                android:textColor="@color/blue"
                android:textSize="18sp"
                tools:ignore="HardcodedText" />

        </androidx.appcompat.widget.Toolbar>


    </com.google.android.material.appbar.AppBarLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="0.7dp"
        android:background="#000"
        app:layout_constraintTop_toBottomOf="@+id/appBarLayout"/>

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="26dp"
        android:layout_height="26dp"
        android:src="@drawable/update_plan"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_marginStart="17.6dp"
        android:layout_marginTop="30dp"
        app:layout_constraintTop_toBottomOf="@+id/appBarLayout" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="19.8dp"
        android:layout_marginStart="18.7dp"
        android:textSize="16sp"
        android:textColor="#000000"
        android:text="Update Plan"
        app:layout_constraintStart_toEndOf="@+id/imageView"
        app:layout_constraintTop_toBottomOf="@+id/appBarLayout"
         />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Update your current plan"
        android:textSize="13sp"
        android:layout_marginTop="6dp"
        android:textColor="#999999"
        android:layout_marginStart="18.7dp"
        app:layout_constraintStart_toEndOf="@+id/imageView"
        app:layout_constraintTop_toBottomOf="@+id/textView2" />

    <View
        android:id="@+id/view"
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:layout_marginTop="14dp"
        android:layout_marginStart="17.6dp"
        android:layout_marginEnd="17.6dp"
        android:background="#999999"

        app:layout_constraintTop_toBottomOf="@+id/textView3"
        tools:layout_editor_absoluteX="16dp" />

    <Button
        android:id="@+id/UpdatePlan"
        android:layout_width="match_parent"
        android:layout_height="69dp"
        android:layout_marginTop="19dp"
        android:background="#00000000"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/appBarLayout"

        />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_marginTop="32dp"
        android:layout_width="26dp"
        android:layout_height="26dp"
        android:src="@drawable/personal_info"
        android:layout_marginStart="18.7dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/view" />

    <TextView
        android:id="@+id/textView11"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Personal Information"
        android:layout_marginTop="18.7dp"
        android:layout_marginStart="18.7dp"
        android:textSize="16sp"
        android:textColor="#000"
        app:layout_constraintStart_toEndOf="@+id/imageView2"
        app:layout_constraintTop_toBottomOf="@+id/view" />

    <TextView
        android:id="@+id/textView13"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Phone no., Address, etc."
        android:layout_marginStart="18.7dp"
        android:textSize="13sp"
        android:layout_marginTop="6dp"
        android:textColor="#999999"
        app:layout_constraintStart_toEndOf="@+id/imageView2"
        app:layout_constraintTop_toBottomOf="@+id/textView11" />

    <View
        android:id="@+id/view2"
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="#999999"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView13"
        android:layout_marginTop="14dp"
        android:layout_marginStart="17.6dp"
        android:layout_marginEnd="17.6dp"

        />

    <Button
        android:id="@+id/PersonalInfo"
        android:layout_width="match_parent"
        android:layout_height="69dp"
        android:layout_marginTop="10dp"
        android:background="#00000000"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/view"

        />


    <ImageView
        android:id="@+id/imageView3"
        android:layout_marginTop="32dp"
        android:layout_width="26dp"
        android:layout_height="26dp"
        android:src="@drawable/invite_friends"
        android:layout_marginStart="18.7dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/view2" />

    <TextView
        android:id="@+id/textView100"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Invite Friends"
        android:layout_marginTop="18.7dp"
        android:layout_marginStart="18.7dp"
        android:textSize="16sp"
        android:textColor="#000"
        app:layout_constraintStart_toEndOf="@+id/imageView3"
        app:layout_constraintTop_toBottomOf="@+id/view2" />

    <TextView
        android:id="@+id/textView130"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Share a link to invite your friends to use Pantomath"
        android:layout_marginEnd="17.6dp"
        android:layout_marginStart="18.7dp"
        android:textSize="13sp"
        android:layout_marginTop="6dp"
        android:textColor="#999999"
        app:layout_constraintStart_toEndOf="@+id/imageView3"
        app:layout_constraintTop_toBottomOf="@+id/textView100" />


    <View
        android:id="@+id/view3"
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="#999999"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView130"
        android:layout_marginTop="14dp"
        android:layout_marginStart="17.6dp"
        android:layout_marginEnd="17.6dp"

        />

    <Button
        android:id="@+id/InviteFriends"
        android:layout_width="match_parent"
        android:layout_height="69dp"
        android:layout_marginTop="8dp"
        android:background="#00000000"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/view2"

        />


    <ImageView
        android:id="@+id/imageView4"
        android:layout_marginTop="32dp"
        android:layout_width="26dp"
        android:layout_height="26dp"
        android:src="@drawable/feedback"
        android:layout_marginStart="18.7dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/view3" />

    <TextView
        android:id="@+id/textView1000"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Feedback"
        android:layout_marginTop="18.7dp"
        android:layout_marginStart="18.7dp"
        android:textSize="16sp"
        android:textColor="#000"
        app:layout_constraintStart_toEndOf="@+id/imageView4"
        app:layout_constraintTop_toBottomOf="@+id/view3" />

    <TextView
        android:id="@+id/textView1300"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Share your feedback about our service"
        android:layout_marginEnd="17.6dp"
        android:layout_marginStart="18.7dp"
        android:textSize="13sp"
        android:layout_marginTop="6dp"
        android:textColor="#999999"
        app:layout_constraintStart_toEndOf="@+id/imageView4"
        app:layout_constraintTop_toBottomOf="@+id/textView1000" />


    <View
        android:id="@+id/view4"
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="#999999"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView1300"
        android:layout_marginTop="14dp"
        android:layout_marginStart="17.6dp"
        android:layout_marginEnd="17.6dp"

        />

    <Button
        android:id="@+id/feedback"
        android:layout_width="match_parent"
        android:layout_height="69dp"
        android:layout_marginTop="5dp"
        android:background="#00000000"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/view3"

        />



    <ImageView
        android:id="@+id/imageView5"
        android:layout_marginTop="32dp"
        android:layout_width="26dp"
        android:layout_height="26dp"
        android:src="@drawable/help"
        android:layout_marginStart="18.7dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/view4" />

    <TextView
        android:id="@+id/textView10000"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Help"
        android:layout_marginTop="18.7dp"
        android:layout_marginStart="18.7dp"
        android:textSize="16sp"
        android:textColor="#000"
        app:layout_constraintStart_toEndOf="@+id/imageView5"
        app:layout_constraintTop_toBottomOf="@+id/view4" />

    <TextView
        android:id="@+id/textView13000"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Share your feedback about our service"
        android:layout_marginEnd="17.6dp"
        android:layout_marginStart="18.7dp"
        android:textSize="13sp"
        android:layout_marginTop="6dp"
        android:textColor="#999999"
        app:layout_constraintStart_toEndOf="@+id/imageView5"
        app:layout_constraintTop_toBottomOf="@+id/textView10000" />


    <View
        android:id="@+id/view5"
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="#999999"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView13000"
        android:layout_marginTop="14dp"
        android:layout_marginStart="17.6dp"
        android:layout_marginEnd="17.6dp"

        />

    <Button
        android:id="@+id/Help"
        android:layout_width="match_parent"
        android:layout_height="69dp"
        android:layout_marginTop="5dp"
        android:background="#00000000"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/view4"

        />

    <TextView
        android:id="@+id/textView14"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="About"
        android:textSize="12sp"
        android:layout_marginStart="17.6dp"
        android:layout_marginTop="14dp"
        android:textColor="#939393"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/view5" />


    <ImageView
        android:id="@+id/imageView6"
        android:layout_marginTop="26dp"
        android:layout_width="26dp"
        android:layout_height="26dp"
        android:src="@drawable/about"
        android:layout_marginStart="18.7dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView14" />


    <TextView
        android:id="@+id/textView100000"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="About Pantomath"
        android:layout_marginTop="26dp"
        android:layout_marginStart="18.7dp"
        android:textSize="16sp"
        android:textColor="#000"
        app:layout_constraintStart_toEndOf="@+id/imageView6"
        app:layout_constraintTop_toBottomOf="@+id/textView14" />




    <View
        android:id="@+id/view6"
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="#999999"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView100000"
        android:layout_marginTop="20dp"
        android:layout_marginStart="17.6dp"
        android:layout_marginEnd="17.6dp"

        />

    <Button
        android:id="@+id/about"
        android:layout_width="match_parent"
        android:layout_height="69dp"
        android:layout_marginTop="28dp"
        android:background="#00000000"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/view5"

        />


    <TextView
        android:id="@+id/textView140"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Logout"
        android:textSize="12sp"
        android:layout_marginStart="17.6dp"
        android:layout_marginTop="14dp"
        android:textColor="#939393"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/view6" />


    <ImageView
        android:id="@+id/imageView7"
        android:layout_marginTop="25dp"
        android:layout_width="26dp"
        android:layout_height="26dp"
        android:src="@drawable/logout"
        android:layout_marginStart="18.7dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView140" />


    <TextView
        android:id="@+id/textView1000000"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Log Out of Pantomath"
        android:layout_marginTop="26dp"
        android:layout_marginStart="18.7dp"
        android:textSize="16sp"
        android:textColor="#FF0000"
        app:layout_constraintStart_toEndOf="@+id/imageView7"
        app:layout_constraintTop_toBottomOf="@+id/textView140" />






    <Button
        android:id="@+id/logout"
        android:layout_width="match_parent"
        android:layout_height="69dp"
        android:layout_marginTop="30dp"
        android:background="#00000000"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/view6"

        />



</androidx.constraintlayout.widget.ConstraintLayout>